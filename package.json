{
  "name": "nextarter-chakra",
  "version": "0.1.0",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:turbo": "npx turbo build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "lint:turbo": "npx turbo lint",
    "type-check": "tsc --noEmit",
    "type-check:turbo": "npx turbo type-check",
    "test:e2e": "playwright test",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.6.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "framer-motion": "^10.12.4",
    "next": "^13.3.1",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.1",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@playwright/test": "^1.33.0",
    "@types/react": "^18.2.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-config-next": "^13.3.1",
    "eslint-config-sznm": "^1.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "next-sitemap": "^4.0.7",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16.x.x",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@8.1.0"
}
